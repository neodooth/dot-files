# dir
alias rmr="rm -r"
alias ll="ls -la"
alias l="ls -lh"
alias lt="ls -lht"
alias lk="clear && ls *"
alias lc="ls|wc -l"
alias ds="du -sh"
alias da="du -sh *"

# programs
alias s="source"
alias g="grep"
alias gv="grep -v"
alias wl="wc -l"
alias v="vim"
alias p="python"
alias p3="python3"
alias pm="python -m"
alias pi="python -i"
alias pcp="python -m cProfile -s cumtime"
alias ipy="ipython"
alias ipyi="ipython -i"
alias act="source bin/activate"
alias dea="deactivate"
alias gv="grep -v"
alias pg="ps -ax|grep"
alias ak="grep -v grep|awk '{print \$2}'|xargs kill"
alias mv="mv -i"
alias ht="htop"

# tmux
alias tm="tmux new -s"
alias tma="tmux attach -t"
alias tml="tmux ls"

# text
alias c="cat"
alias cn="cat -n"
alias t="tail"
alias tn="tail -n"
alias t1="tail -n 1"
alias tf="tail -f"
alias h="head"
alias hn="head -n"
alias h1="head -n 1"
