# dir
alias rmr="rm -r"
alias ll="ls -la"
alias l="ls -lh"
alias lt="ls -lht"
alias lk="clear && ls *"
alias lc="ls|wc -l"
alias ds="du -sh"
alias da="du -sh *"

# programs
alias jarp="jar -tf"
alias ma="make all"
alias mca="make clean && make all"
alias gas="./gradlew assemble"
alias s="source"
alias g="grep"
alias gv="grep -v"
alias wl="wc -l"
alias v="vim"
alias p="python3"
alias p3="python3"
alias p2="python2"
alias act="source bin/activate"
alias vact="source venv/bin/activate"
alias dea="deactivate"
alias gv="grep -v"
alias pg="ps aux|grep"
alias ak="grep -v grep|awk '{print \$2}'|xargs kill"
alias k="kill"
alias k9="kill -9"
alias mv="mv -i"

# tmux
alias ts="tmux capture-pane -pJS -100000"
alias tm="tmux new -s"
alias tma="tmux attach -t"
alias tml="tmux ls"

# text
alias c="cat"
alias cn="cat -n"
alias t="tail"
alias tn="tail -n"
alias t1="tail -n 1"
alias tf="tail -f"
alias h="head"
alias hn="head -n"
alias h1="head -n 1"

